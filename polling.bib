@article{Blais2002,
  title = {Why Is There so Little Strategic Voting in {{Canadian}} Plurality Rule Elections?},
  author = {Blais, Andr{\'e}},
  year = {2002},
  journal = {Political Studies},
  volume = {50},
  number = {3},
  pages = {445--454},
  issn = {00323217},
  doi = {10.1111/1467-9248.00378},
  abstract = {Using the 1988 Canadian Election Study I examine why there was only restricted strategic voting in single-member district plurality elections. In that election 19 percent of Canadian voters preferred the party that actually finished third in their constituency, but among these third party supporters only one in eight decided to vote strategically for one of the top two contenders. Strategic voting was relatively rare for two key reasons. First, many third party supporters had a strong preference for their party over all others and were therefore reluctant to rally to either of the top two contenders. Second, many overestimated their party's chance of winning and as a consequence did not feel that their vote would be wasted.}
}

@article{Blais2003a,
  title = {Why Is Turnout Higher in Some Countries than in Others?},
  author = {Blais, Andr{\'e} and Massicotte, Louis and Dobrzynska, Agnieszka},
  year = {2003},
  number = {March},
  pages = {1--26},
  issn = {0304-4130},
  doi = {10.1111/1475-6765.00382},
  abstract = {The purpose of this study was to determine the impact of certain institutional variables on\textbackslash nelectoral participation. The study covers a total of 151 elections held in 61 ``democratic''\textbackslash ncountries since 1990. The analysis is based on two measurements of voter turnout, using either\textbackslash nthe population of voting age or the number of people registered on the electoral lists as the\textbackslash ndenominator.\textbackslash nThe research shows that:\textbackslash n1. voter turnout is a dozen points higher in countries where voting is compulsory, provided\textbackslash nthere is a penalty for failing to vote;\textbackslash n2. turnout is 5 to 6 points higher in countries where the electoral system is proportional or\textbackslash nmixed compensatory;\textbackslash n3. turnout is some 10 points higher in countries where it is possible to vote by mail, in advance\textbackslash nor by proxy than in countries where none of these options are available.\textbackslash nThe data do not show, however, a systematic effect of elector registration methods. Nor does it\textbackslash nappear that voter turnout is any higher in countries where polling day is a holiday.\textbackslash nIt can be concluded that institutional factors such as compulsory voting and the voting system\textbackslash naffect voter turnout, but so do administrative measures that determine how easy it is to vote.}
}

