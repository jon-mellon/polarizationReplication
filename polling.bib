  author = {Curtice, John and Steed, Michael},
  year = {1986},
  journal = {Electoral Studies},
  volume = {5},
  number = {3},
  pages = {209--228},
  issn = {02613794},
  doi = {10.1016/0261-3794(86)90012-0},
  abstract = {The British electoral system has traditionally been defended on the grounds that its systematic exaggeration of the lead of the Conservatives over Labour or vice versa secured for the largest party an overall majority of seats on a minority of the votes and thereby enabled the electorate directly to determine which party holds office. Recent changes in the electoral geography of Britain have eroded that systematic exaggeration at Westminster elections and may soon remove it entirely. Meanwhile the electoral system is discouraging the pursuit of geographically aggregative policies. Consequently, the single member plurality system is no longer a suitable instrument to achieve the ends of its defenders but, despite their availability, alternative systems which could meet those aims have not been seriously considered. \textcopyright{} 1986.}
}

