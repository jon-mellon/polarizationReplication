@article{Facchini2012,
  title = {Individual Attitudes towards Skilled Migration: {{An}} Empirical Analysis across Countries},
  author = {Facchini, Giovanni and Mayda, Anna Maria},
  year = {2012},
  journal = {World Economy},
  volume = {35},
  number = {2},
