@article{Antonucci2017a,
  title = {The Malaise of the Squeezed Middle: {{Challenging}} the Narrative of the `left behind' {{Brexiter}}},
  author = {Antonucci, Lorenza and Horvath, Laszlo and Kutiyski, Yordan and Krouwel, Andr{\'e}},
