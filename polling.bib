@article{Arzheimer2015,
  title = {Another Dog That Didn't Bark? {{Partisan}} de-Alignment and Voting in the 2013 Election},
  author = {Arzheimer, Kai},
  year = {2015},
  month = jan,
  journal = {German Politics},
  volume = {4008},
  number = {February},
  pages = {49--64},
  publisher = {{Taylor \& Francis}},
  issn = {0964-4008},
  doi = {10.1080/09644008.2016.1266481},
  abstract = {De-alignment, a loosening of traditional ties between the two major parties and their voters has been a longstanding issue in German Politics for the last three decades or so. The decline of class and religion, the expansion of (higher) education and the transformation of the media system have slowly eroded partisanship since its heyday in the 1070s \textendash{} or so the story goes. In recent years, however, this decline has halted, and the two parties most closely associated with traditional partisanship are now jointly governing Germany. This article takes a fresh look at the development of party identification in Germany over the last decade and tries to disentangle the somewhat paradoxical role partisan ties played in bringing about the 2013 result. In this context, the loyalties of (former) SPD supporters, who are still deeply disappointed about the party's `neo-liberal' turn in the early 2000s, are of particular interest.}
}

