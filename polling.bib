@article{Klandermans2008,
  title = {Embeddedness and Identity: {{How}} Immigrants Turn Grievances into Action},
  author = {Klandermans, B. and {van der Toorn}, J. and {van Stekelenburg}, J.},
  year = {2008},
  journal = {American Sociological Review},
  volume = {73},
  number = {6},
  pages = {992--1012},
  issn = {0003-1224},
  doi = {10.1177/000312240807300606},
  abstract = {The social and political integration of Muslim immigrants into Western societies is among the most pressing problems of today. Research documents how immigrant communities are increasingly under pressure to assimilate to their ``host'' societies in the face of significant discrimination. In this article, we bring together two literatures\textemdash that on immigrants and that on social movement participation\textemdash to explore whether Muslim immigrants respond to their societal situation by engaging in collective political action. Although neither literature has given much attention to immigrant collective action, they do provide predictive leverage relative to the influence of grievances, efficacy, identity, emotions, and embeddedness in civil society networks. Our analyses are comprised of three separate but identical studies: a study of Turkish (N = 126) and Moroccan immigrants (N = 80) in the Netherlands and a study of Turkish immigrants (N = 100) in New York. Results suggest that social psychological mechanisms known to affect native citizens' collective action function similarly for immigrants to a great extent, although certain immigrant patterns are indeed unique. [ABSTRACT FROM AUTHOR]},
