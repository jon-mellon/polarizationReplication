@article{Hanretty2018,
  title = {Comparing Strategies for Estimating Constituency Opinion from National Survey Samples},
  author = {Hanretty, Chris and Lauderdale, Benjamin E. and Vivyan, Nick},
  year = {2018},
  journal = {Political Science Research and Methods},
  volume = {6},
  number = {3},
  pages = {571--591},
  issn = {20498489},
  doi = {10.1017/psrm.2015.79},
  abstract = {{$<$}p{$>$}Political scientists interested in estimating how public opinion varies by constituency have developed several strategies for supplementing limited constituency survey data with additional sources of information. We present two evaluation studies in the previously unexamined context of British constituency-level opinion: an external validation study of party vote share in the 2010 general election and a cross-validation of opinion toward the European Union. We find that most of the gains over direct estimation come from the inclusion of constituency-level predictors, which are also the easiest source of additional information to incorporate. Individual-level predictors combined with post-stratification particularly improve estimates from unrepresentative samples, and geographic local smoothing can compensate for weak constituency-level predictors. We argue that these findings are likely to be representative of applications of these methods where the number of constituencies is large.{$<$}/p{$>$}},
  isbn = {2049-8470},
  pmid = {10949896}
}

@article{hanrettyChoiceBasedMeasure2020,
  title = {A {{Choice}}-{{Based}} Measure of Issue Importance in the Electorate},
  author = {Hanretty, Chris and Lauderdale, Benjamin E. and Vivyan, Nick},
  year = {2020},
  month = feb,
  journal = {American Journal of Political Science},
  pages = {ajps.12470},
  publisher = {{Wiley}},
  issn = {0092-5853},
  doi = {10.1111/ajps.12470},
  abstract = {Measuring how much citizens care about di\"i\textquestiondown\textquestiondown erent policy issues is critical for political scientists, yet existing measurement approaches have signi\"i\textquestiondown\textquestiondown cant limitations. We provide a new survey-experimental, choice-based approach for measuring the importance voters attach to di\"i\textquestiondown\textquestiondown erent positional issues, including issues not currently contested by political elites. We combine information from (i) direct questions eliciting respondents' positions on di\"i\textquestiondown\textquestiondown erent issues with (ii) a conjoint experiment asking respondents to trade-o\"i\textquestiondown\textquestiondown{} departures from their preferred positions on those issues. Applying this method to study the relative importance of \"i\textquestiondown\textquestiondown\"i\textquestiondown\textquestiondown{} issues in the UK, we show that British voters attach signi\"i\textquestiondown\textquestiondown cant importance to issues like the death penalty which are not presently the subject of political debate and attach more importance to those issues associated with social liberal-conservative rather than economic left-right divisions.}
}

