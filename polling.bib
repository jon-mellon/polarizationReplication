@article{Lanoue1992,
  title = {The Sources of Tactical Voting in {{British}} Parliamentary Elections, 1983-1987},
  author = {Lanoue, David J. and Bowler, Shaun},
  year = {1992},
  journal = {Political Behavior},
  volume = {14},
