@article{Harell2012,
  title = {The Impact of Economic and Cultural Cues on Support for Immigration in Canada and the United States},
  author = {Harell, Allison and Soroka, Stuart N. and Iyengar, Shanto and Valentino, Nicholas A.},
  year = {2012},
