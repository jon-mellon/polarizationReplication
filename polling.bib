@article{Wronski2018,
  title = {A Tale of Two Democrats: {{How}} Authoritarianism Divides the Democratic Party},
  author = {Wronski, Julie and Levitan, Lindsey C. and Johnson, April A. and Bankert, Alexa and Amira, Karyn},
  year = {2018},
  journal = {The Journal of Politics},
  volume = {80},
  number = {4},
  pages = {1384--1388},
  issn = {0022-3816},
  doi = {10.1086/699338},
  abstract = {Authoritarianism has been predominantly used in American politics as a predictor of Republican identification and conservative policy preferences. We argue that this approach has neglected the role authoritarianism plays among Democrats and how it can operate within political parties regardless of their ideological orientation. Drawing from three distinct sets of data, we demonstrate the impact of authoritarianism in the 2016 Democratic Party's primaries. Authoritarianism consistently predicts differences in primary voting among Democrats, particularly support for Hillary Clinton over Bernie Sanders. This effect is robust across various model specifications including controls for ideology, partisan strength, and other predispositions. These results highlight the potential of authoritarianism to shape leadership preferences within the Democratic Party. We advocate for a reconsideration of authoritarianism as a disposition with meaningful consequences for intraparty dynamics and conclude with practical implications regarding the future of the Democratic Party.}
}

