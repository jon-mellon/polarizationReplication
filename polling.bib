@book{Levendusky2009,
  title = {The Partisan Sort: {{How}} Liberals Became Democrats and Conservatives Became Republicans},
  author = {Levendusky, Matthew S.},
  year = {2009},
  pages = {1--200},
  publisher = {{University of Chicago Press}},
  issn = {0033-362X},
  doi = {10.1093/poq/nfq050},
  abstract = {As Washington elites drifted toward ideological poles over the past few decades, did ordinary Americans follow their lead? In The Partisan Sort, Matthew Levendusky reveals that we have responded to this trend\textemdash but not, for the most part, by becoming more extreme ourselves. While polarization has filtered down to a small minority of voters, it also has had the more significant effect of reconfiguring the way we sort ourselves into political parties. In a marked realignment since the 1970s\textemdash when partisan affiliation did not depend on ideology and both major parties had strong liberal and conservative factions\textemdash liberals today overwhelmingly identify with Democrats, as conservatives do with Republicans. This ``sorting'', Levendusky contends, results directly from the increasingly polarized terms in which political leaders define their parties. Exploring its far-reaching implications for the American political landscape, he demonstrates that sorting makes voters more loyally partisan, allowing campaigns to focus more attention on mobilizing committed supporters. Ultimately, Levendusky concludes, this new link between party and ideology represents a sea change in American politics.},
  isbn = {978-0-226-47365-9},
  pmid = {15706511},
  keywords = {Political parties—United States.,United States—Politics and government—2001–,Voting research—United States.}
}

